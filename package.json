{
  "name": "the-first-drop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:base && npm run lint:css",
    "lint:base": "next lint",
    "lint:css": "stylelint '**/*.css'",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|ts|tsx|json)\"",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch",
    "test:coverage": "CI=true npm test -- --env=jsdom --coverage",
    "test:update": "jest --updateSnapshot",
    "test:compact": "jest 2>&1 | grep 'FAIL' | sort -t: -u -k1,1",
    "commit:check": "npm run lint && npm run test && npm run build",
    "dep:update": "./node_modules/.bin/npm-check -u"
  },
  "dependencies": {
    "next": "12.3.1",
    "postcss": "^8.4.17",
    "postcss-modules-values": "^4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.2.0",
    "@testing-library/user-event": "14.2.0",
    "@types/react": "18.0.9",
    "babel-jest": "^27.4.6",
    "eslint-config-next": "12.3.1",
    "eslint-plugin-testing-library": "^5.7.2",
    "jest": "28.1.0",
    "jest-environment-jsdom": "28.1.0",
    "npm-check": "^6.0.1",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "stylelint": "^14.13.0",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^9.0.0",
    "testing-library": "^0.0.2"
  }
}
